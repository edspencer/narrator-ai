{
  "name": "narrator-ai",
  "version": "0.2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "npx jest"
  },
  "devDependencies": {
    "@narrator-ai/eslint-config": "workspace:*",
    "@narrator-ai/tsconfig": "workspace:*",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.10",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "leaked-handles": "^5.2.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.63",
    "ai": "^3.4.7",
    "react": "^18.2.0",
    "readline": "^1.3.0",
    "winston": "^3.14.2",
    "yaml": "^2.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
